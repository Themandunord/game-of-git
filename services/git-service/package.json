{
	"name": "git-service",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"license": "MIT",
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
		"start:debug": "nodemon --config nodemon-debug.json",
		"prestart:prod": "rimraf dist && npm run build",
		"start:prod": "node dist/main.js",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"start:dev2": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
		"start:dev3": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" "
	},
	"dependencies": {
		"@nestjs/common": "^6.0.0",
		"@nestjs/core": "^6.0.0",
		"@nestjs/cqrs": "^6.0.1",
		"@nestjs/graphql": "^6.2.4",
		"@nestjs/jwt": "6.1.1",
		"@nestjs/mongoose": "^6.1.2",
		"@nestjs/passport": "^6.1.0",
		"@nestjs/platform-express": "^6.0.0",
		"@pulumi/aws": "^0.18.20",
		"@pulumi/awsx": "^0.18.6",
		"@pulumi/pulumi": "^0.17.21",
		"apollo-server-express": "^2.6.3",
		"class-transformer": "^0.2.3",
		"graphql-subscriptions": "^1.1.0",
		"husky": "^3.0.2",
		"jsonwebtoken": "^8.5.1",
		"lint-staged": "^9.2.1",
		"lodash": ">=4.17.13",
		"mongoose": "^5.6.4",
		"passport": "^0.4.0",
		"passport-http-bearer": "^1.0.1",
		"passport-jwt": "^4.0.0",
		"prisma-binding": "^2.3.13",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^2.6.2",
		"rxjs": "^6.5.2",
		"type-graphql": "^0.17.4",
		"vuetify": "^1.5.5",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@nestjs/testing": "^6.0.0",
		"@types/express": "^4.16.0",
		"@types/jest": "^23.3.13",
		"@types/mongoose": "^5.5.8",
		"@types/node": "^12.6.2",
		"@types/supertest": "^2.0.7",
		"concurrently": "^4.1.0",
		"jest": "^23.6.0",
		"mongodb-memory-server": "^5.1.5",
		"nodemon": "^1.18.9",
		"prettier": "^1.15.3",
		"stylus": "^0.54.5",
		"supertest": "^3.4.1",
		"ts-jest": "24.0.2",
		"ts-node": "8.1.0",
		"tsc-watch": "^4.0.0",
		"tsconfig-paths": "3.8.0",
		"tslint": "5.16.0",
		"typescript": "3.4.3",
		"wait-on": "^3.2.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
